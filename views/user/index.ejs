                <div class="row">
					  <div class="col-lg-6">
                       
                        <p><a href="/user/create" class="btn btn-info">Добавить</a></p>
                        
                        
                        <div class="table-responsive">
                            <table class="table table-bordered table-hover table-striped">
                                <thead>
                                    <tr>
                                        <th>Email</th>
                                        <th>Имя</th>
                                        <th>Телефон</th>
                                        <th>Действия</th>
                                    </tr>
                                </thead>
                                <tbody>
                                   <% _.each(users, function (user) { %>
                                    <tr>
                                        <td><a href="mailto:<%= user.email %>"><%= user.email %></a></td>
                                        <td><%= user.userName %></td>
                                        <td><%= user.phone %></td>
                                        <td>
                                            <a class="btn btn-info" href="/user/view/<%= user.id %>">Edit</a>                                                                     <a class="btn btn-warning" href="/user/delete/<%= user.id %>">Delete</a>
                                        </td>
                                    </tr>
                                    <%}) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
				</div>
				<!-- /.row -->
